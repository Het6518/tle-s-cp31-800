#include<bits/stdc++.h> // Header file that includes all standard C++ libraries
using namespace std;

int main()
{
    int t; // Variable to store the number of test cases
    cin >> t; // Input the number of test cases

    // Loop through each test case
    while(t--)
    {
        int n; // Variable to store the value for the current test case
        cin >> n; // Input the value of n

        // Check the condition to determine the winner
        // If n+1 is divisible by 3 or n-1 is divisible by 3, "First" wins
        if(((n + 1) % 3 == 0) || ((n - 1) % 3 == 0)) 
            cout << "First" << endl; // Print "First" if the condition is true
        else 
            cout << "Second" << endl; // Otherwise, print "Second"
    }

    return 0; // Indicate successful program termination
}
